<%= form_with model: [:admin, @pipeline] do |f| %>
  <%= render 'shared/error_messages', target: @pipeline %>
  <%= f.text_field :name %>
  <%= f.text_area :description %>
  <%= f.select :piping_type, options_for_select(Pipeline.options_i18n(:piping_type), @pipeline.piping_type) %>
  <%= f.submit %>
<% end %>
