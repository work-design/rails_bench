<%= form_with model: [:admin, @pipeline_member] do |f| %>
  <%= f.text_field :name %>
  <%= f.collection_select :job_title_id, @job_titles, :id, :name, { include_blank: true }, { data: { url: members_admin_pipeline_url(@pipeline_member.pipeline_id) } } %>
  <%= f.select :member_id, options_for_select([]) %>
  <%= f.submit %>
<% end %>
