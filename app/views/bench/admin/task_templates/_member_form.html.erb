<%= form_with model: @task_template, url: { action: 'update' } do |f| %>
  <%=
    f.collection_select(
        :job_title_id,
        @job_titles,
        :id, :name, { include_blank: true },
        { data: { controller: 'typer', url: members_admin_project_taxon_task_templates_path(@project_taxon) } }
    )
  %>
  <%=
    f.collection_select(
        :member_id,
        @members,
        :id, :name, include_blank: true
    )
  %>
  <%= f.color_field :color %>
  <%= f.submit %>
<% end %>
