<%= render layout: 'index_table', locals: { cache_key: ProjectMilepost.column_names.hash } do %>
  <%= render partial: 'index_tbody', collection: @project.project_mileposts.reject(&->(i){i.id.nil?}), as: :project_milepost %>
<% end %>
<%= form_with model: @project_milepost, url: { action: 'create' } do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= f.submit %>
<% end %>
