<tr>
  <td class="has-text-right"><%= Bench::Project.human_attribute_name(:name) %></td>
  <td><%= @project.name %></td>
</tr>
<tr>
  <td class="has-text-right"><%= Bench::Project.human_attribute_name(:created_at) %></td>
  <td><time data-controller="time"><%= @project.created_at.to_formatted_s(:iso8601) %></time></td>
</tr>
<% @project.extra.each do |k, v| %>
<tr>
  <td class="has-text-right"><%= k.to_s %></td>
  <td><%= v %></td>
</tr>
<% end %>
