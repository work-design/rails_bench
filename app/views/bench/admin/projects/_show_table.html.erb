<tr>
  <td class="has-text-right"><%= Project.human_attribute_name(:creator_id) %></td>
  <td>
    <% @project.owners.each do |owner| %>
      <p><%= owner.name %></p>
    <% end %>
  </td>
</tr>
<% @project.extra.each do |k, v| %>
<tr>
  <td class="has-text-right"><%= k.to_s %></td>
  <td><%= v %></td>
</tr>
<% end %>
