<%= form_with model: @project_facilitate, url: admin_project_project_facilitates_path(@project) do |f| %>
  <%= render 'shared/error_messages', target: @project_facilitate %>
  <%=
  f.collection_select(
      :facilitate_id,
      @facilitates,
      :id, :name, { include_blank: true },
      { data: { controller: 'typer', url: providers_admin_project_project_facilitates_path(@project) } }
  )
%>
<%=
  f.collection_select(
      :provider_id,
      @providers,
      :id, :name
  )
%>
  <%= f.submit %>
<% end %>
