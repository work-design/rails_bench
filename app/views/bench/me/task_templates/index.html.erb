<%= render partial: 'project_bar', locals: { project: @project } %>

<div class="ui divided items" id="task_templates">
  <% @task_templates.each do |task_template| %>
    <%= render partial: 'index_item', locals: { task_template: task_template } %>
  <% end %>
  <div id="item_add" class="item">
    <%= link_to new_me_project_task_template_path(@project), remote: true do %>
      <i class="fas fa-plus"></i>
    <% end %>
  </div>
</div>
