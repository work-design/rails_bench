<%= form_with model: [:me, @project, @expense] do |f| %>
  <%= render 'shared/error_messages', target: @expense %>
  <%= f.text_field :subject %>
  <%= f.text_field :amount %>
  <%= f.text_area :note %>
  <%= f.text_field :financial_taxon_id %>
  <%= f.file_field :proof %>
  <%= f.submit %>
<% end %>
