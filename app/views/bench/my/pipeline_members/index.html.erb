<p id="notice"><%= notice %></p>

<h1>Pipeline Members</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pipeline_members.each do |pipeline_member| %>
      <tr>
        <td><%= link_to 'Show', pipeline_member %></td>
        <td><%= link_to 'Edit', edit_pipeline_member_path(pipeline_member) %></td>
        <td><%= link_to 'Destroy', pipeline_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pipeline Member', new_pipeline_member_path %>
